{
  "name": "ts-attempt",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node src/app.ts",
    "prod": "npm run build && node ./dist/app.js",
    "build": "npm run generate-licenses && tsc && npm run copy-assets",
    "generate-licenses": "./node_modules/.bin/generate-license-file --input ./package.json --output src/assets/3rd-party-licenses.txt",
    "copy-assets": "./node_modules/.bin/copyfiles -u 1 \"./src/assets/**\" dist/",
    "debug": "npm run build && node --nolazy --inspect-brk=9229 dist/src/app.js",
    "lint-fix": "tslint --fix src/**/*.ts",
    "test": "NODE_ENV=test && ./node_modules/mocha/bin/mocha --require ts-node/register 'tests/**/*.test.ts' --ui tdd",
    "coverage": "NODE_ENV=test && nyc --reporter=html --reporter=text-summary --all ./node_modules/mocha/bin/mocha --require ts-node/register 'tests/**/*.test.ts' --ui tdd",
    "generate-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n Migration",
    "update-database": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.6.14",
    "@overnightjs/logger": "^1.1.10",
    "@types/connect-mongodb-session": "0.0.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-mongodb-session": "^2.3.1",
    "cookie-parser": "^1.4.4",
    "copyfiles": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "generate-license-file": "^1.1.0",
    "http-status-codes": "^1.4.0",
    "joi": "^14.3.1",
    "joiful": "^2.0.0",
    "jwt-simple": "^0.5.6",
    "mssql": "^6.1.0",
    "nodemailer": "^6.4.6",
    "nyc": "^15.0.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.1.0",
    "typemoq": "^2.1.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jwt-simple": "^0.5.33",
    "@types/nodemailer": "^6.4.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.9",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/joi": "^14.3.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.7.1",
    "@types/passport": "^1.0.2",
    "@types/passport-github": "^1.1.5",
    "@types/passport-local": "^1.0.33",
    "@types/validator": "^12.0.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0"
  }
}
